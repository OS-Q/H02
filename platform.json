  },
  "packageRepositories": [
    "http://182.61.61.133/link/toolchain/manifest.json",
    "http://182.61.61.133/link/toolchain/openocd.json",
    "http://182.61.61.133/link/framework/manifest.json",
    "http://182.61.61.133/link/platformio.json"
  ],
